package Csla;

public interface FilterProviderListener {
	  /**
	   * Defines the method signature for a filter provider method used by FilteredBindingList.
	   * @param item The object to be filtered.
	   * @param The filter criteria.
	   * @return true if the item matches the filter.
	   */
	  public boolean onFilterProviderInvoked(Object item, Object filter);
}
